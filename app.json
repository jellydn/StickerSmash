{
	"expo": {
		"name": "StickerSmash",
		"scheme": "stickersmash",
		"slug": "StickerSmash",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#FDF6EA"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FDF6EA"
			}
		},
		"web": {
			"bundler": "metro",
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-router"],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "bf36a687-1bf4-4b1c-89de-271591d8a152"
			}
		},
		"owner": "dunghd"
	}
}
